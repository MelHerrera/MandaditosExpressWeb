<template id="tipodepago-modal-template">
    <div class="row">
        <div class="col-md-8">
            <div class="form-group">
                <label for="Descripcion" class="control-label">Descripción</label>
                <input type="text" data-bind="value:Descripcion, disable:$root.Disable" name="Descripcion" class="form-control" />
            </div>
        </div>
    </div>
    <div class="row">

        <div class="col-md-10">
            <div class="form-group">
                <label for="Estado" class="control-label">Estado</label>
                <input type="checkbox" data-bind="checked: EstadoTipoDePago,bsSwitchButton: {}, disable:$root.Disable" name="EstadoTipoDePago" id="switch-estadotipodepago" >
            </div>
        </div>

    </div>
</template>

