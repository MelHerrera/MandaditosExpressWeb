<template id="Velocidad-modal-template">

    <div class="row">
        <div class="col-md-6">
            <div class="form-group">
                <label for="Descripcion" class="control-label">Descripcion</label>
                <input type="text" data-bind="value:Descripcion" name="Descripcion" class="form-control" />
            </div>
        </div>
        
    </div>
    <div class="row">
        <div class="col-md-10">
            <div class="form-group">
                <label for="Estado" class="control-label">Estado</label>
                <input type="checkbox" data-bind="checked: Estado, bsSwitchButton: {}" data-toggle="switchbutton" data-onstyle="info" name="Estado" id="switch-estado">
            </div>
        </div>
    </div>
</template>

